<div class="pull-right">
  {% if project.medias %}
    <div class="carousel-container">
    {% if project.medias|length > 0 %}
      {% include 'BrickstormSolidRBundle:Project:_carousel.html.twig' with {'medias': project.medias, 'id': project.id} %}
    {% elseif project.parent %}
      {% include 'BrickstormSolidRBundle:Project:_carousel.html.twig' with {'medias': project.parent.medias, 'id': project.id} %}
    {% endif%}
    </div>
  {% endif %}
  <br/>
  {% if map is defined %}
    {{ google_map_container(map) }}
    {{ google_map_js(map) }}
  {% endif %}
</div>